{
  "name": "@nlui/react-ui",
  "version": "0.2.0",
  "description": "NLUI React UI Components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup && npm run build:dts",
    "build:dts": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nlui",
    "react",
    "ui",
    "components",
    "chat",
    "ai"
  ],
  "author": "ZacharyZcR",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@nlui/client": "workspace:*",
    "@nlui/react": "workspace:*",
    "md-editor-rt": "^6.3.1",
    "lucide-react": "^0.564.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
